<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <VBox fx:id="controler" blendMode="MULTIPLY" layoutY="-1.0" prefHeight="640.0" prefWidth="260.0">
            <children>
                <HBox prefHeight="72.0" prefWidth="260.0" />
                <VBox prefHeight="67.0" prefWidth="260.0">
                    <children>
                        <Label text="Chọn kiểu lau">
                            <VBox.margin>
                                <Insets bottom="5.0" left="7.0" top="20.0" />
                            </VBox.margin>
                        </Label>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <RadioButton fx:id="radioOnline" mnemonicParsing="false" selected="true" text="Online">
                                    <HBox.margin>
                                        <Insets left="20.0" top="7.0" />
                                    </HBox.margin>
                                </RadioButton>
                                <RadioButton fx:id="radioOffline" mnemonicParsing="false" text="Offline">
                                    <HBox.margin>
                                        <Insets left="80.0" top="7.0" />
                                    </HBox.margin>
                                </RadioButton>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <VBox prefHeight="58.0" prefWidth="260.0">
                    <children>
                        <Label text="Tự động sinh Map">
                            <VBox.margin>
                                <Insets left="7.0" />
                            </VBox.margin>
                        </Label>
                  <Button fx:id="randomMap" mnemonicParsing="false" text="Map Random">
                     <VBox.margin>
                        <Insets left="20.0" top="7.0" />
                     </VBox.margin>
                  </Button>
                    </children>
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </VBox>
                <HBox prefHeight="37.0" prefWidth="260.0">
                    <children>
                        <Label text="Bắt đầu / Dừng">
                            <HBox.margin>
                                <Insets left="7.0" top="13.0" />
                            </HBox.margin>
                        </Label>
                        <ToggleButton fx:id="pauseRobot" mnemonicParsing="false" text="Start">
                            <HBox.margin>
                                <Insets left="90.0" />
                            </HBox.margin>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </ToggleButton>
                    </children>
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </HBox>
            <VBox prefHeight="59.0" prefWidth="260.0">
               <children>
                   <RadioButton fx:id="hardDirt" mnemonicParsing="false" text="Chất bẩn cứng đầu">
                       <VBox.margin>
                           <Insets left="20.0" top="7.0" />
                       </VBox.margin>
                   </RadioButton>
                  <RadioButton fx:id="radioRoboot" mnemonicParsing="false" selected="true" text="Robot">
                     <VBox.margin>
                        <Insets left="20.0" top="7.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="movingTrap" mnemonicParsing="false" text="Vật cản di động">
                     <VBox.margin>
                        <Insets left="20.0" top="7.0" />
                     </VBox.margin>
                  </RadioButton>
               </children>
            </VBox>
            </children></VBox>
        <!-- Phần Map chính-->
        <Pane fx:id="map" layoutX="260.0" prefHeight="600.0" prefWidth="600.0" />
    </children>
</AnchorPane>
